{
  "family": "travel-mate-server",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "essential": true,
      "memory": 256,
      "name": "travel-mate-server",
      "cpu": 1,
      "image": "885729917217.dkr.ecr.eu-north-1.amazonaws.com/travel-mate-server:latest",
      "logConfiguration": {
        "logDriver": "awslogs",
        "secretOptions": null,
        "options": {
          "awslogs-group": "/ecs/travel-mate-server",
          "awslogs-region": "eu-north-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "portMappings": [
        {
          "hostPort": 8080,
          "protocol": "tcp",
          "containerPort": 8080
        }
      ],
      "environment": [
        {
          "name": "SRV_HOST",
          "value": "0.0.0.0"
        },
        {
          "name": "SRV_PORT",
          "value": "8079"
        },
        {
          "name": "RUST_LOG",
          "value": "INFO"
        }
      ]
    }
  ]
}
